#compare key to items on the left of it
#if key is less , insert it

def InsertionSort(arr):
      for i in range(len(arr)):
          key = arr[i]
          j = i -1
      while j >= 0 and arr[j] > key:
            arr[j + 1] = arr[j]
            j -= 1
        
        # Insert the key into its correct position within the sorted portion
        arr[j + 1] = key
    
    return arr

#time: O(n2)
#space: O(1)
