t = int(input())

for _ in range(t):
    n = int(input())
    volumes = list(map(int, input().split()))
    
        
    count = False
    for i in range(1, n):
        if volumes[i] >= volumes[i-1]:
            count = True
            break
        
    
    if count :
        print("YES")
    else:
        print("NO")
